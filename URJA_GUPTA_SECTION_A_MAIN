    "\n",
    "# Time to maturity\n",
    "洧녢 = 1\n",
    "\n",
    "# Evenly sampled Stock price at time 洧노 (in $)\n",
    "St = np.linspace(1, 100, 100)\n",
    "\n",
    "fC = np.vectorize(C)\n",
    "fP = np.vectorize(P)"
   ]
