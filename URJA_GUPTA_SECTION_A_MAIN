    "import numpy as np\n",
    "import scipy as s\n",
    "import sympy as sy\n",
    "from sympy import *\n",
    "from scipy import integrate\n",
    "import math\n",
    "from math import exp, pi, sqrt, log\n",
    "from scipy.integrate import quad\n",
    "from scipy.stats import norm\n",
    "from scipy.misc import derivative\n",
    "\n",
    "plt.rcParams.update(\n",
    "    {\n",
    "        'text.usetex': False,\n",
    "        'font.family': 'stixgeneral',\n",
    "        'mathtext.fontset': 'stix',\n",
    "    }\n",
    ")\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 837,
   "id": "52b08c5b",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Defining functions to analyse Black-Scholes-Merton Model\n",
    "\n",
    "def d1(x):\n",
    "    return (((log(x/k)) + (r + ((洧랥**2)/2)*(T-t)))/(洧랥*(sqrt(T-t))))\n",
    "\n",
    "def d2(x):\n",
    "    return (d1(x) - 洧랥*(sqrt(T-t)))\n",
    "\n",
    "# CDF of the standard normal distribution\n",
    "def N(x):\n",
    "    return s.stats.norm.cdf(x)\n",
    "\n",
    "#  BSM price of a call option at time 洧노 and stock price 洧녡洧노 (in $)\n",
    "def C(x):\n",
    "    return ((N(d1(x)))*x - (N(d2(x)))*k*exp(-r*(T-t)))\n",
    "\n",
    "#  BSM price of a put option at time 洧노 and stock price 洧녡洧노(in $)\n",
    "def P(x):\n",
    "    return N(-d2(x))*k*exp((-r*(T-t))) - N(-d1(x))*x"
   ]
  },
  {
   "cell_type": "markdown",
   "id": "2b28b94a",
   "metadata": {},
   "source": [
    "Q3. b."
   ]
  },
